<h1>Import XML-data</h1>
<%= form_tag 'institution/import', method: :post, remote: true, multipart: true, authenticity_token: true do %>
    <label for="file">Upload text File</label> <%= file_field_tag "file" %>
    <%= token_tag form_authenticity_token %>
    <%= submit_tag %>
<% end %>

<%= link_to 'Back', institutions_path %>
